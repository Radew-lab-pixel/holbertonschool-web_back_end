// Write a script that adds a new attribute to a document in the collection school:
// db.school.update({name: "Holberton school"}, {$set: {address: "972 Mission street"}});
db.createCollection("school");
db.school.insert({name:"Holberton school"})
db.school.updateMany({name: "Holberton school"}, {$set: {address: "972 Mission street"}});

